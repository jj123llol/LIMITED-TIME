
function teleportTO(placeCFrame)
    local plyr = game.Players.LocalPlayer;
    if plyr.Character then
        plyr.Character.HumanoidRootPart.CFrame = placeCFrame;
    end
end

local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Limited time -by jude ", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

local Tab = Window:MakeTab({
	Name = "Win minigames",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})
local Section = Tab:AddSection({
	Name = "Games"
})
local Section = Tab:AddSection({
	Name = "PRESS AFTER TIME STARTS MOVING"
})

Tab:AddButton({
	Name = "Truss mayhem",
	Callback = function()
      	teleportTO(game:GetService("Workspace").Minigames["Truss mayhem"].End.CFrame)
          wait(1)
      	teleportTO(game:GetService("Workspace").Booths.Booth.Base.CFrame)
  	end    
})
Tab:AddButton({
	Name = "Block climb",
	Callback = function()
      	teleportTO(game:GetService("Workspace").Minigames["Block climb"].End.CFrame)
      	wait(1)
      	teleportTO(game:GetService("Workspace").Booths.Booth.Base.CFrame)
  	end    
})
