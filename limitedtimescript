local Tab = Window:MakeTab({
	Name = "Fun",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})
local Section = Tab:AddSection({
	Name = "Fun stuff"
})
Tab:AddButton({
	Name = "Make everyone your avatar",
	Callback = function()
       OrionLib:MakeNotification({
	Name = "If it did not work",
	Content = "Equip bodyswap potion from shop and hold it,then press again",
	Image = "rbxassetid://4483345998",
	Time = 10
})
wait(0.5)
      	
          for i,v in pairs(game.Players:GetChildren()) do

local args = {
    [1] = v.Character
}

game:GetService("Players").LocalPlayer.Character:FindFirstChild("Body Swap").SwapRemote:FireServer(unpack(args))
end



  	end    
})
